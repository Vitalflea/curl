<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>curl-static</id>
    <version>1.0.5</version>
    <authors>Vitalflea</authors>
    <owners>Vitalflea</owners>
    <projectUrl>https://github.com/Vitalflea/curl</projectUrl>
    <icon>curl.png</icon>
    <description>Pre-compiled libcurl binaries for vc143. I made this because any other packages did not work for me!</description>
    <tags>libcurl C++</tags>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="curl.png" target="curl.png" />
    <file src="include\**\*" target="build\native\include" />
    <file src="lib\**\*" target="build\native\lib" />
    <file src="curl-static.targets" target="build\native" />
  </files>
</package>
